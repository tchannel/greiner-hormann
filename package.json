{
  "name": "greiner-hormann",
  "version": "1.0.0",
  "description": "Methods for spatial operations on (Multi)Polygons",
  "main": "index.js",
  "scripts": {
    "test": "node test/test.js",
    "doc": "dox -r < index.js | doxme --readme > README.md",
    "build-debug": "browserify index.js --debug --standalone greiner-hormann | exorcist dist/greiner-hormann.js.map > dist/greiner-hormann.js",
    "build-min": "browserify index.js --standalone greiner-hormann | uglifyjs -c -m > dist/greiner-hormann.min.js",
    "build": "npm run build-debug && npm run build-min"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/tchannel/greiner-hormann.git"
  },
  "keywords": [
    "gis",
    "union",
    "difference",
    "intersect",
    "polygon",
    "clipping"
  ],
  "author": "Tim Channell",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tchannel/greiner-hormann/issues"
  },
  "homepage": "https://github.com/tchannel/greiner-hormann",
  "devDependencies": {
    "benchmark": "^1.0.0",
    "browserify": "^9.0.3",
    "dox": "^0.6.1",
    "doxme": "^1.4.2",
    "glob": "~4.3.1",
    "tape": "~3.0.3",
    "uglify-js": "^2.4.16"
  },
  "dependencies": {
    "turf-inside": "^1.1.3",
    "turf-is-clockwise": "^1.0.0",
    "turf-point": "^2.0.0",
    "turf-polygon": "^1.0.2"
  }
}
